<div class="modal fade" id="adminSharedModal" tabindex="-1" aria-labelledby="adminSharedModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="adminSharedModalLabel"></h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <div class="admin-modal-body-content"></div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-outline-secondary" data-bs-dismiss="modal">@SharedLocalizer["Cancel"]</button>
                <button type="button" class="btn btn-outline-danger btn-admin-modal-confirm" onclick="window._adminSharedModalConfirm()"></button>
            </div>
        </div>
    </div>
</div>
